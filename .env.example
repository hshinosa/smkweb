APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info
LOG_DAILY_DAYS=30

# Database Configuration
# Default to pgsql for production features (pgvector)
DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5433
DB_DATABASE=sman1_baleendah
DB_USERNAME=sman1_user
DB_PASSWORD=sman1_password_2024

# Session & Cache
SESSION_DRIVER=redis
CACHE_STORE=redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
CACHE_PREFIX=sman1_baleendah_cache_

MEMCACHED_HOST=127.0.0.1

# Redis Configuration
# For LOCAL development (running outside Docker): Use localhost with mapped port
# For DOCKER/PRODUCTION: Use service name 'redis' with internal port 6379
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=32768
REDIS_DB=0
REDIS_CACHE_DB=1
REDIS_PREFIX=sman1_baleendah_

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="info@sman1baleendah.sch.id"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"

# CDN Configuration (Cloudflare)
ASSET_URL="${APP_URL}"
CDN_URL="${APP_URL}"

# AI Configuration (Groq)
# Get API keys from https://console.groq.com/keys
# Multiple keys can be configured in Admin > AI Settings for round-robin
GROQ_API_KEY=

# Self-hosted Embedding Service (Docker TEI)
EMBEDDING_PROVIDER=tei
EMBEDDING_BASE_URL=http://embedding:8080
EMBEDDING_MODEL=intfloat/multilingual-e5-small
EMBEDDING_DIMENSIONS=384

# Instagram Scraper
INSTAGRAM_SCRAPER_SLEEP=21600 # 6 hours
DB_PORT_INTERNAL=5432
